<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="250dp"
        android:height="60dp">
        <shape>
            <solid android:color="@color/scheme8" />
        </shape>
    </item>

    <item
        android:width="43dp"
        android:height="42dp"
        android:left="250dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="0"
            android:pivotY="0">
            <shape>
                <solid android:color="@color/scheme8" />
            </shape>
        </rotate>
    </item>

</layer-list>
